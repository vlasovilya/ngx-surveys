{
  "name": "ngx-surveys",
  "version": "0.9.19",
  "description": "Angular 8+ survey / form builder",
  "bugs": "https://github.com/vlasovilya/ngx-surveys/issues",
  "homepage": "https://github.com/vlasovilya/ngx-surveys#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlasovilya/ngx-surveys.git"
  },
  "license": "MIT",
  "author": "Ilya Vlasov",
  "private": true,
  "keywords": [
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "ngx",
    "ng2",
    "angular",
    "form",
    "survey",
    "form-builder",
    "surveys"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rimraf dist/ngx-surveys && ng build ngx-surveys --configuration production",
    "build:demo": "rimraf dist/ngx-surveys-demo && ng build ngx-surveys-demo --configuration production --base-href=/ngx-surveys/demo/",
    "release": "cd dist/ngx-surveys && yarn publish",
    "bundlewatch": "bundlewatch",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "cleanup": "rimraf build dist waste deploy demo docs",
    "lint": "ng lint",
    "lint:ngx-surveys": "ng lint ngx-surveys --type-check",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=protractor-ci.conf.js",
    "typedoc": "typedoc --options typedoc.json src/lib/*.ts && ts-node -P tsconfig.build.json docs.ts"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/animations": "^18.2.12",
    "@angular/cdk": "^18.2.13",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/material": "^18.2.13",
    "@angular/material-moment-adapter": "^18.2.13",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/router": "^18.2.12",
    "core-js": "^2.5.1",
    "lodash": "^4.17.4",
    "moment": "2.29.1",
    "ngx-file-drop": "^16.0.0",
    "ngx-mask": "^13.1.12",
    "recordrtc": "^5.6.2",
    "rxjs": "^6.5.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "wavesurfer.js": "^6.1.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@angular/language-service": "^18.2.12",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "all-contributors-cli": "^5.10.0",
    "bundlewatch": "^0.2.5",
    "codelyzer": "^6.0.0",
    "copy": "^0.3.1",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^0.52.1",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typedoc": "^0.14.2",
    "typescript": "~5.4.5"
  },
  "angular-cli": {},
  "bundlewatch": [
    {
      "path": "./dist/packages-dist/ngx-survey.umd.js",
      "maxSize": "8 kB"
    }
  ]
}
