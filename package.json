{
  "name": "ngx-surveys",
  "version": "0.1.1",
  "description": "Angular 8+ survey / form builder",
  "bugs": "https://github.com/vlasovilya/ngx-surveys/issues",
  "homepage": "https://github.com/vlasovilya/ngx-surveys#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlasovilya/ngx-surveys.git"
  },
  "license": "MIT",
  "author": "Ilya Vlasov",
  "private": true,
  "keywords": [
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "ngx",
    "ng2",
    "angular",
    "form",
    "survey",
    "form-builder",
    "surveys"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "rimraf dist && ts-node -P tsconfig.build.json build.ts",
    "build:demo": "rimraf demo && ng build --aot --prod --base-href=/ngx-surveys/demo/ --output-path=demo",
    "release": "cd dist/packages-dist && yarn publish",
    "bundlewatch": "bundlewatch",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "cleanup": "rimraf build dist waste deploy demo docs",
    "lint": "ng lint",
    "lint:ngx-surveys": "ng lint ngx-surveys --type-check",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=protractor-ci.conf.js",
    "typedoc": "typedoc --options typedoc.json src/lib/*.ts && ts-node -P tsconfig.build.json docs.ts"
  },
  "dependencies": {
    "@angular/animations": "^11.2.5",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "^11.2.5",
    "@angular/compiler": "^11.2.5",
    "@angular/core": "^11.2.5",
    "@angular/forms": "^11.2.5",
    "@angular/material": "^10.0.0",
    "@angular/material-moment-adapter": "^11.2.1",
    "@angular/platform-browser": "^11.2.5",
    "@angular/platform-browser-dynamic": "^11.2.5",
    "@angular/router": "^11.2.5",
    "core-js": "^2.5.1",
    "lodash": "^4.17.4",
    "moment": "2.18.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "^10.0.0",
    "@angular/compiler-cli": "^11.2.5",
    "@angular/language-service": "^11.2.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "all-contributors-cli": "^5.10.0",
    "bundlewatch": "^0.2.5",
    "codelyzer": "^6.0.0",
    "copy": "^0.3.1",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^5.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^0.52.1",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typedoc": "^0.14.2",
    "typescript": "~4.1.5"
  },
  "angular-cli": {},
  "bundlewatch": [
    {
      "path": "./dist/packages-dist/ngx-survey.umd.js",
      "maxSize": "8 kB"
    }
  ]
}
